{
  "name": "@anchor-chat/api",
  "version": "1.2.5",
  "description": "A package that allows you to communicate with the Anchor Chat",
  "main": "src/index.js",
  "browser": "src/browser.js",
  "types": "src/index.d.ts",
  "scripts": {
    "dist": "npm run build && npm run docs",
    "build": "gulp browserify",
    "serve": "npx http-server",
    "docs": "mkdir docs && typedoc --out docs --mode modules ./src/",
    "test": "mocha --timeout 200000",
    "lint": "eslint . --fix",
    "prepare": "require-self"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anchor-Chat/AnchorAPI.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "anchor",
    "chat",
    "api"
  ],
  "author": "lukas2005",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/Anchor-Chat/AnchorAPI/issues"
  },
  "homepage": "https://github.com/Anchor-Chat/AnchorAPI#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "mocha": "^7.2.0",
    "require-self": "^0.2.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "browser-keygen": "^1.0.5",
    "core-js": "^3.6.5",
    "crypto-browserify": "^3.12.0",
    "eventemitter3": "^4.0.4",
    "ipfs": "^0.45.0",
    "orbit-db": "^0.24.1",
    "orbit-db-access-controllers": "^0.2.4",
    "regenerator-runtime": "^0.13.5",
    "uninstall": "0.0.0",
    "uuid": "^3.4.0"
  },
  "dependenciesMeta": {
    "browser-keygen": {
      "unplugged": true
    }
  }
}
